base:def-composition a mymath:Definition ;
    rdfs:label "Definition: Composition"@en,
        "定義: 合成"@en ;
    rdfs:comment "**Composition** is the fundamental operation in a [Category](def-category.qmd) that combines two compatible [Morphisms](def-morphism.qmd) to produce a third morphism."@en,
        "**合成**は[圏](def-category.qmd)における基本的な操作であり、互換性のある2つの[射](def-morphism.qmd)を結合して第3の射を生成します。"@en ;
    mymath:hasDomain "Category Theory",
        "圏論" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-category,
        base:def-morphism .

