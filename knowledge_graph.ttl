base:def-associativity a mymath:Definition ;
    rdfs:label "Definition: Associativity"@en ;
    rdfs:comment "**Associativity** is a fundamental property of a @def-binary-operation that allows us to perform multiple operations without needing to specify the order of evaluation using parentheses."@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation,
        base:def-group .

base:def-binomial-coefficient a mymath:Definition ;
    rdfs:label "Definition: Binomial Coefficient"@en ;
    mymath:hasDomain "Combinatorics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-combination,
        base:def-permutation .

base:def-combination a mymath:Definition ;
    rdfs:label "Definition: Combination"@en ;
    mymath:hasDomain "Combinatorics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-set .

base:def-complete-metric-space a mymath:Definition ;
    rdfs:label "Definition: Complete Metric Space"@en ;
    mymath:hasDomain "Analysis" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-limit,
        base:def-metric-space,
        base:ex-real-line-metric .

base:def-composition a mymath:Definition ;
    rdfs:label "Definition: Composition"@en ;
    rdfs:comment "**Composition** is the fundamental operation in a @def-category that combines two compatible @def-morphism[s] to produce a third morphism."@en ;
    mymath:hasDomain "Category Theory" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-category,
        base:def-isomorphism,
        base:def-morphism .

base:def-conditional-probability a mymath:Definition ;
    rdfs:label "Definition: Conditional Probability"@en ;
    rdfs:comment "The **conditional probability** of an @def-event $A$ given event $B$ is the probability that $A$ occurs, given that $B$ has occurred. It is denoted $P(A|B)$."@en ;
    mymath:hasDomain "Probability Statistics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-event,
        base:def-probability-space,
        base:def-random-variable,
        base:thm-bayes .

base:def-connected a mymath:Definition ;
    rdfs:label "Definition: Connected Space"@en ;
    mymath:hasDomain "Topology" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/Mathlib.Topology.Connected.IsConnected> ;
    mymath:uses base:def-closed-set,
        base:def-open-set,
        base:def-topological-space .

base:def-convergence a mymath:Definition ;
    rdfs:label "Definition: Convergence"@en ;
    rdfs:comment "**Convergence** describes the behavior of a @def-sequence whose terms approach a specific value as the index increases. A convergent sequence gets arbitrarily close to its limit."@en ;
    mymath:hasDomain "Analysis" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-complete-metric-space,
        base:def-limit,
        base:def-sequence .

base:def-expectation a mymath:Definition ;
    rdfs:label "Definition: Expected Value"@en ;
    mymath:hasDomain "Probability Statistics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-probability-space,
        base:def-random-variable .

base:def-gcd a mymath:Definition ;
    rdfs:label "Definition: Greatest Common Divisor"@en ;
    mymath:hasDomain "Number Theory" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-set .

base:def-hausdorff a mymath:Definition ;
    rdfs:label "Definition: Hausdorff Space"@en ;
    mymath:hasDomain "Topology" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/Mathlib.Topology.Separation.T2Space> ;
    mymath:uses base:def-open-set,
        base:def-topological-space,
        base:thm-metric-hausdorff .

base:def-homomorphism a mymath:Definition ;
    rdfs:label "Definition: Group Homomorphism"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation,
        base:def-group .

base:def-identity-element a mymath:Definition ;
    rdfs:label "Definition: Identity Element"@en ;
    rdfs:comment "An **identity element** for a @def-binary-operation is an element that leaves any other element unchanged when combined with it."@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation,
        base:def-group,
        base:thm-unique-identity .

base:def-image a mymath:Definition ;
    rdfs:label "Definition: Image"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/Mathlib.LinearAlgebra.LinearMap.range> ;
    mymath:uses base:def-linear-transformation,
        base:def-vector-space .

base:def-isomorphism a mymath:Definition ;
    rdfs:label "Definition: Isomorphism"@en ;
    rdfs:comment "An **isomorphism** in a @def-category is a @def-morphism that has a two-sided inverse. Isomorphic objects are considered \"the same\" from the categorical perspective."@en ;
    mymath:hasDomain "Category Theory" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-category,
        base:def-composition,
        base:def-morphism,
        base:def-natural-transformation .

base:def-kernel a mymath:Definition ;
    rdfs:label "Definition: Kernel"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/Mathlib.LinearAlgebra.LinearMap.ker> ;
    mymath:uses base:def-linear-transformation,
        base:def-vector-space .

base:def-matrix a mymath:Definition ;
    rdfs:label "Definition: Matrix"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" .

base:def-permutation a mymath:Definition ;
    rdfs:label "Definition: Permutation"@en ;
    mymath:hasDomain "Combinatorics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-set .

base:def-sample-space a mymath:Definition ;
    rdfs:label "Definition: Sample Space"@en ;
    rdfs:comment "A **sample space** is the @def-set of all possible outcomes of a random experiment or probabilistic scenario. It is typically denoted by $\\Omega$ (omega) or $S$."@en ;
    mymath:hasDomain "Probability Statistics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-event,
        base:def-probability-space,
        base:def-set .

base:def-sequence a mymath:Definition ;
    rdfs:label "Definition: Sequence"@en ;
    rdfs:comment "A **sequence** is an ordered list of elements, typically numbers, indexed by the natural numbers. Formally, a sequence is a function from the natural numbers to a set."@en ;
    mymath:hasDomain "Analysis" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-convergence,
        base:def-limit,
        base:def-set .

base:def-triangle a mymath:Definition ;
    rdfs:label "Definition: Triangle"@en ;
    rdfs:comment "A **triangle** is a polygon with three vertices and three sides. It is one of the fundamental shapes in geometry, formed by connecting three non-collinear @def-point[s]."@en ;
    mymath:hasDomain "Geometry" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-angle,
        base:def-line,
        base:def-point,
        base:thm-angle-sum-triangle,
        base:thm-pythagorean .

base:ex-integers-addition a mymath:Example ;
    rdfs:label "Example: Integers under Addition"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation,
        base:def-group,
        base:def-set .

base:formal-proof-def-group a mymath:FormalProof ;
    rdfs:label "Formal proof of def-group"@en ;
    rdfs:comment "Implemented in MathKnowledgeGraph.Algebra.Groups"@en ;
    mymath:inModule "Mathlib.Algebra.Group.Defs.Group" .

base:formal-proof-def-ring a mymath:FormalProof ;
    rdfs:label "Formal proof of def-ring"@en ;
    rdfs:comment "Implemented in MathKnowledgeGraph.Algebra.Rings"@en ;
    mymath:inModule "Mathlib.Algebra.Ring.Defs.Ring" .

base:thm-angle-sum-triangle a mymath:Theorem ;
    rdfs:label "Theorem: Angle Sum of a Triangle"@en ;
    rdfs:comment "The sum of the interior @def-angle[s] of any @def-triangle equals $180°$ (or $\\pi$ radians)."@en ;
    mymath:hasDomain "Geometry" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-angle,
        base:def-line,
        base:def-triangle .

base:thm-bayes a mymath:Theorem ;
    rdfs:label "Theorem: Bayes' Theorem"@en ;
    rdfs:comment "**Bayes' Theorem** provides a way to calculate @def-conditional-probability in the \"reverse\" direction. It relates $P(A|B)$ to $P(B|A)$, allowing us to update beliefs based on new evidence."@en ;
    mymath:hasDomain "Probability Statistics" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-conditional-probability,
        base:def-event .

base:thm-heine-borel a mymath:Theorem ;
    rdfs:label "Theorem: Heine-Borel Theorem"@en ;
    mymath:hasDomain "Topology" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-closed-set,
        base:def-compact,
        base:def-metric-space .

base:thm-pythagorean a mymath:Theorem ;
    rdfs:label "Theorem: Pythagorean Theorem"@en ;
    mymath:hasDomain "Geometry" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-euclidean-space .

base:thm-unique-identity a mymath:Theorem ;
    rdfs:label "Theorem: Uniqueness of Identity"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-group .

base:def-angle a mymath:Definition ;
    rdfs:label "Definition: Angle"@en ;
    rdfs:comment "An **angle** is the figure formed by two rays (half-lines) that share a common endpoint. The common endpoint is called the **vertex** of the angle, and the two rays are called the **sides** of the angle."@en ;
    mymath:hasDomain "Geometry" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-euclidean-space,
        base:def-line,
        base:def-point .

base:def-basis a mymath:Definition ;
    rdfs:label "Definition: Basis"@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
