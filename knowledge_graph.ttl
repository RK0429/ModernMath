@prefix base: <https://mathwiki.org/resource/> .
@prefix mymath: <https://mathwiki.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://mathwiki.org/ontology> a owl:Ontology ;
    rdfs:label "Mathematics Knowledge Graph Ontology"@en ;
    rdfs:comment "An ontology for representing mathematical knowledge as a graph"@en .

base:ax-induction a mymath:Axiom ;
    rdfs:label "Axiom: Mathematical Induction"@en ;
    mymath:hasDomain "Logic Set Theory" ;
    mymath:hasStatus "complete" .

base:def-commutativity a mymath:Definition ;
    rdfs:label "Definition: Commutativity"@en ;
    rdfs:comment "**Commutativity** is a property of a @def-binary-operation where the order of the operands does not affect the result."@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-associativity,
        base:def-binary-operation,
        base:def-group .

base:def-distributivity a mymath:Definition ;
    rdfs:label "Definition: Distributivity"@en ;
    rdfs:comment "**Distributivity** is a property that relates two @def-binary-operation[s], describing how one operation \"distributes\" over the other."@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation .

base:def-integral a mymath:Definition ;
    rdfs:label "Definition: Integral"@en ;
    rdfs:comment "The **integral** of a function represents the signed area under its curve, accumulation of quantities, or the inverse operation to differentiation. We focus on the Riemann integral for real-valued functions."@en ;
    mymath:hasDomain "Analysis" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-continuity,
        base:def-limit .

base:def-inverse-element a mymath:Definition ;
    rdfs:label "Definition: Inverse Element"@en ;
    rdfs:comment "An **inverse element** of an element under a @def-binary-operation is another element that, when combined with the original, yields the @def-identity-element."@en ;
    mymath:hasDomain "Algebra" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation,
        base:def-group,
        base:def-identity-element .

base:def-parallel a mymath:Definition ;
    rdfs:label "Definition: Parallel Lines"@en ;
    rdfs:comment "Two @def-line[s] are **parallel** if they lie in the same plane and do not intersect, no matter how far they are extended."@en ;
    mymath:hasDomain "Geometry" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-line,
        base:def-point .

base:ex-closed-interval-compact a mymath:Example ;
    rdfs:label "Example: Closed Interval is Compact"@en ;
    mymath:hasDomain "Topology" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-compact,
        base:def-topological-space,
        base:thm-heine-borel .

