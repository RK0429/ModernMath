        base:def-random-variable .

base:thm-limit-arithmetic a mymath:Theorem ;
    rdfs:label "Theorem: Arithmetic of Limits"@en ;
    mymath:hasDomain "Analysis" ;
    mymath:hasStatus "complete" ;
