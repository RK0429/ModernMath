        base:def-vector-space .

base:thm-mean-value a mymath:Theorem ;
    rdfs:label "Theorem: Mean Value Theorem"@en ;
    rdfs:comment "The **Mean Value Theorem (MVT)** states that for a differentiable function on an interval, there exists at least one point where the instantaneous rate of change equals the average rate of change over the interval."@en ;
    mymath:hasDomain "Analysis" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-continuity,
        base:def-derivative .

base:thm-pigeonhole-principle a mymath:Theorem ;
    rdfs:label "Theorem: Pigeonhole Principle"@en ;
