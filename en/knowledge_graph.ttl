    rdfs:label "Mathematics Knowledge Graph Ontology"@en ;
    rdfs:comment "An ontology for representing mathematical knowledge as a graph"@en .

base:ax-induction a mymath:Axiom ;
    rdfs:label "Axiom: Mathematical Induction"@en,
        "公理: 数学的帰納法"@en ;
    mymath:hasDomain "Logic Set Theory",
        "論理・集合論" ;
    mymath:hasStatus "complete" .

base:def-complete-metric-space a mymath:Definition ;
    rdfs:label "Definition: Complete Metric Space"@en,
        "定義: 完備距離空間"@en ;
    mymath:hasDomain "Analysis",
        "解析学" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-limit,
        base:def-metric-space .

base:def-convergence a mymath:Definition ;
    rdfs:label "Definition: Convergence"@en,
        "定義: 収束"@en ;
    rdfs:comment "**Convergence** describes the behavior of a [Sequence](def-sequence.qmd) whose terms approach a specific value as the index increases. A convergent sequence gets arbitrarily close to its limit."@en,
        "**収束**とは、[数列](def-sequence.qmd)の項が、添字が増加するにつれて特定の値に近づく挙動を表す。収束する数列は、その極限に任意に近づく。"@en ;
    mymath:hasDomain "Analysis",
        "解析学" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-limit,
        base:def-sequence .

base:def-distributivity a mymath:Definition ;
    rdfs:label "Definition: Distributivity"@en,
        "定義: 分配性"@en ;
    rdfs:comment "**Distributivity** is a property that relates two [Binary Operations](def-binary-operation.qmd), describing how one operation \"distributes\" over the other."@en,
        "**分配性**は、2つの[二項演算](def-binary-operation.qmd)を関連付ける性質で、一方の演算が他方に対してどのように「分配」されるかを記述する。"@en ;
    mymath:hasDomain "Algebra",
        "代数学" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-binary-operation .

base:def-group-example a mymath:Definition ;
    rdfs:label "Definition: Group"@en,
        "定義：群"@en ;
    mymath:hasDomain "Algebra",
        "代数学" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-associativity,
        base:def-binary-operation,
        base:def-homomorphism,
        base:def-identity-element,
        base:def-inverse-element,
        base:def-subgroup,
        base:ex-integers-addition,
        base:thm-lagrange,
        base:thm-unique-identity .

base:def-integral a mymath:Definition ;
    rdfs:label "Definition: Integral"@en,
        "定義: 積分"@en ;
    rdfs:comment "The **integral** of a function represents the signed area under its curve, accumulation of quantities, or the inverse operation to differentiation. We focus on the Riemann integral for real-valued functions."@en,
        "関数の**積分**は、その曲線の下の符号付き面積、量の累積、または微分の逆演算を表す。ここでは実数値関数のリーマン積分に焦点を当てる。"@en ;
    mymath:hasDomain "Analysis",
        "解析学" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-continuity,
        base:def-limit .

base:def-isomorphism a mymath:Definition ;
    rdfs:label "Definition: Isomorphism"@en,
        "定義: 同型射"@en ;
    rdfs:comment "An **isomorphism** in a [Category](def-category.qmd) is a [Morphism](def-morphism.qmd) that has a two-sided inverse. Isomorphic objects are considered \"the same\" from the categorical perspective."@en,
        "[圏](def-category.qmd)における**同型射**は、両側逆射を持つ[射](def-morphism.qmd)です。同型な対象は圏論的観点から「同じ」と見なされます。"@en ;
    mymath:hasDomain "Category Theory",
        "圏論" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-category,
        base:def-composition,
        base:def-morphism .

base:def-parallel a mymath:Definition ;
    rdfs:label "Definition: Parallel Lines"@en,
        "定義: 平行線"@en ;
    rdfs:comment "2つの[直線](def-line.qmd)は、同一平面上にあり、どれだけ延長しても交わらないとき**平行**であるといいます。"@en,
        "Two [Lines](def-line.qmd) are **parallel** if they lie in the same plane and do not intersect, no matter how far they are extended."@en ;
    mymath:hasDomain "Geometry",
        "幾何学" ;
    mymath:hasStatus "complete" ;
    mymath:uses base:def-line,
        base:def-point .

