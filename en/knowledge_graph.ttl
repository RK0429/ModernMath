base:def-vector-space a mymath:Definition ;
    rdfs:label "Definition: Vector Space"@en,
        "定義: ベクトル空間"@en ;
    mymath:hasDomain "Algebra",
        "代数学" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/Mathlib.LinearAlgebra.Basic.Module> .

base:def-binary-operation a mymath:Definition ;
    rdfs:label "Definition: Binary Operation"@en,
        "定義: 二項演算"@en ;
    mymath:hasDomain "Algebra",
        "代数学" ;
    mymath:hasLeanId "MathKnowledgeGraph.Algebra.BinaryOp" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/MathKnowledgeGraph.Algebra.BinaryOp> ;
    mymath:uses base:def-set .

base:def-group a mymath:Definition ;
    rdfs:label "Definition: Group"@en,
        "群"@en ;
    mymath:hasDomain "Algebra",
        "代数学" ;
    mymath:hasLeanId "Mathlib.Algebra.Group.Defs",
        "Mathlib.Algebra.Group.Defs.Group" ;
    mymath:hasStatus "complete" ;
    mymath:isVerifiedBy <https://mathlib.org/proof/Mathlib.Algebra.Group.Defs.Group>,
        base:formal-proof-def-group ;
    mymath:uses base:def-associativity,
        base:def-binary-operation,
        base:def-commutativity,
        base:def-homomorphism,
        base:def-identity-element,
        base:def-inverse-element,
        base:def-set,
        base:def-subgroup,
        base:ex-even-integers-subgroup,
        base:ex-finite-field,
        base:ex-integers-addition,
        base:thm-lagrange,
        base:thm-unique-identity .

base:def-set a mymath:Definition ;
    rdfs:label "Definition: Set"@en,
        "定義: 集合"@en ;
    mymath:hasDomain "Logic Set Theory",
        "論理・集合論" ;
    mymath:hasStatus "complete" .

